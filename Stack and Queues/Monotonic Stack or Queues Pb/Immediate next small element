public class Solution {
    public static void immediateSmaller(int []a) {
        // Write your code here.
        int n= a.length;
        //int nse[] = new nse[n];
       // Stack<Integer> st = new Stack<>();
        for(int i=0;i<n-1;i++){
            int j=i+1;
            if(a[j]>a[i]){
                a[i]=-1;
            }
            else{
                a[i]=a[j];

            }
        }
        a[n-1]=-1;

    }
}
